/* Move down content because we have a fixed navbar that is 50px tall */
body {
    padding-top: 50px;
    padding-bottom: 20px;
    background-color:#fff;
    font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif !important;
    /*background:url(../Images/bodyBG.jpg) left top transparent;*/
    background:url(../Images/bodyBG.png) left top transparent;
   
}
a{-webkit-transition: color 300ms ease-in-out 50ms;
    transition: color 300ms ease-in-out 50ms;}
a:hover
{-webkit-transition: color 300ms ease-in-out 50ms;
    transition: color 300ms ease-in-out 50ms;}
.body-content
{
    margin-top:26px;
}
.navbar
{
    min-height:75px;
}
.navbar-brand
{
    padding-top:5px;

}
.Dash_ContentTable
{
    width:100%;
}
.main_box_in
{
    /*background-color:#ffc208;*/
    background-color:#ef646a;
    padding:0px;
    border-radius:0.2em;
    -moz-border-radius:0.2em;
    -webkit-border-radius:0.2em;
    font-size:1em;
    color:#a07800;
    margin:10px 0px 5px;
    height:34px;
    float:left;
    width:100%;
}
.closeBtn {
    background: url(../Images/close.png) left top no-repeat transparent;
    width: 32px;
    height: 32px;
    border: 0px none #000;
    margin-left: 8px;
    margin-right: 10px;
    cursor: pointer;
    float: right;
}

    .closeBtn:hover {
        background: url(../Images/close.png) left -51px no-repeat transparent;
        cursor: pointer;
    }

/*=================================GRID STYLES==================================*/
.Grid
{
    float:left;
    width:100%;
}
.gridOuterDiv
{
    float:left;
    width:99.6%;
    border-top:1px solid #c4c4c4;
    border-bottom:1px solid #c4c4c4;
    border-collapse:collapse;
   
padding:0px;
margin:2px;
background-color:#fff;

}
.gridTransparentDiv
{
    float:left;
    width:99.6%;
    border-top:1px solid #c4c4c4;
    border-bottom:1px solid #c4c4c4;
    border-collapse:collapse;
   
padding:5px 0px;
margin:2px;
background-color:transparent;

}
.smallGridContainer {
    float:left;
    background-color:#f4f4f4;
    border:1px dotted #e0e0e0;
    padding:2px;
    width:100%;
    min-height:10px;
    border-radius:0.3em;
}
.gridOuterDiv .gridtable
{
    border-collapse:collapse;
    width:100%;
    border:0px none #fff;
    margin-top:0em;
    margin-bottom:0em;
     border-radius:0.4em;
    -webkit-border-radius:0.4em;
    -moz-border-radius:0.4em;
}
.gridtable input
{
    border: 1px solid #d4d4d4;
    background: #fff;
    color: #333;
    font-size: 1em;
    margin: 5px 0px 6px;
    padding: 3px 5px;
    width: 96%;
    background:url(../Images/InputBGSmall.png) repeat-x left bottom #fff;
    height:26px;
    line-height:26px;
    border-radius:0.2em;
    -moz-border-radius:0.2em;
    -webkit-border-radius:0.2em;
    color:#797979;
}

.gridtable th
{
   background: #D4D4D4 url("../Images/gridHeaderBG.png") repeat-x scroll left bottom;
min-height: 24px;
vertical-align: middle;
line-height: 24px;
color: #444;
font-size: 0.9em;
font-weight: bold;
padding: 5px 2px;
border-width: 0px 0px 1px;
border-style: none none solid;
border-color: #FFF #FFF #C4C4C4;
-moz-border-top-colors: none;
-moz-border-right-colors: none;
-moz-border-bottom-colors: none;
-moz-border-left-colors: none;
border-image: none;
text-transform: uppercase !important;
}
.gridtable th a {
    color:#638db7 !important;
    font-size:1em !important;
    font-weight:bold !important;
}


.GridHeader {
}

.sortasc a {
        display: block;
        padding: 0 4px 0 15px;
        background: url(../Images/asc.gif) no-repeat;
    }

.sortdesc a {
        display: block;
        padding: 0 4px 0 15px;
        background: url(../Images/desc.gif) no-repeat;
    }

.sortupdown a {
        display: block;
        padding: 0 4px 0 15px;
        background: url(../Images/images_updown.png) no-repeat;
    }

.gridtable tr
{
    background-color: #FFF;
border-bottom: 1px solid #d4d4d4;
padding: 0px;
color: #5E5E5E;
font-size: 0.8em;
border-top: 0px none #fff;
 border-radius:0.4em;
    -webkit-border-radius:0.4em;
    -moz-border-radius:0.4em;
}
.gridtable tr td
{
    padding:4px 2px;
    border:0px solid #d4d4d4;
    font-size:0.85em;
}
    .gridtable tr td img {
        padding:5px;
        border:1px dashed #c0c0c0;
    }
.gridtable tr td a
{
    color:#32c6ec;
    text-decoration:none;
}
    .gridtable tr td a:hover {
        color:#5e5e5e;
        background-color:#fff;
        border-radius:0.4em;
        -moz-border-radius:0.4em;
        -webkit-border-radius:0.4em;
    }
.gridtable tr .LastTD
{
    padding:4px 2px;
    border-right:0px none #c6d5e1;
    width:7%;
}
.gridtable tr .SpaceTD
{
    width:12%;
}
.gridtable tr .bigSpaceTD
{
    width:21%;
}
.gridtable tr .smallTD
{
    width:8%;
}
.gridtable .alternateTR
{
    background-color: #f0f0f0;
}
.gridtable tr:nth-child(odd){ 
		background: #f0f0f0;
	}
	/*  Define the background color for all the EVEN background rows  */
.gridtable tr:nth-child(even){
		background: #fff;
	} 
.crossRemove {
    width:16px !important;
    height:16px !important;
    float:left !important;
    margin-left:2px !important;
    border:0px none #fff !important;
    cursor:pointer !important;
    background:url(../Images/cross-script.png) no-repeat left top transparent !important;
}
.crossRemove:hover {
    background: url(../Images/cross-script.png) no-repeat left -16px transparent !important;
    width:16px !important;
    height:16px !important;
    float:left !important;
    margin-left:2px !important;
    border:0px none #fff !important;
    cursor:pointer !important;
}
.editGrid {
    background: url(../Images/editGrid.png) no-repeat left top transparent !important;
    width:16px !important;
    height:16px !important;
    float:left !important;
    margin-right:2px !important;
    border:0px none #fff !important;
    cursor:pointer !important;
}
.editGrid:hover {
    background: url(../Images/editGrid.png) no-repeat left -24px transparent !important;
    width:16px !important;
    height:16px !important;
    float:left !important;
    margin-right:2px !important;
    border:0px none #fff !important;
    cursor:pointer !important;
}

